#include <bits/stdc++.h>
using namespace std;
#define lli long long int
lli n, m,d,s;
int main()
{
    ifstream fin("cowtip.in");
    ofstream fout("cowtip.out");
    int  a[10][10];
    fin>>n;
    for(int i=0;i<n;i++){
        string x;
        fin>>x;
        for(int j=0;j<x.length();j++){
            a[i][j] = x[j] - 48;
        }
    }
    int ans = 0;
    for(int i=n-1;i>=0;i--){
        for(int j = n-1;j>=0;j--){
            if(a[i][j] == 1){
                ans++;
                for(int k = 0;k<=i;k++){
                    for(int h = 0;h<=j;h++){
                        if(a[k][h]==1) a[k][h]=0;
                        else a[k][h] = 1;
                    }
                }
            }
        }
    }
    fout<<ans<<endl;


}
