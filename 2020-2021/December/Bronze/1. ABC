#include <bits/stdc++.h>

using namespace std;
#define l long long int
int main()
{
    int n[7];
    for(int i = 0;i<7;i++){
        cin>>n[i];
    }
    sort(n, n+7);
    int a = n[0];
    int b = n[1];
    l sum =0;
    for(int i = 2; i<7;i++){
        sum+=n[i];
    }
    int c = (sum- 3*(a+b))/4;
    cout<<a<< " "<<b<< " "<<c<<endl;
}
