#include <bits/stdc++.h>
using namespace std;
#define lli long long int
lli n, m,d,s;
int main()
{
    ifstream fin("highcard.in");
    ofstream fout("highcard.out");
    fin>>n;
    int total[100001]  ={0};
    vector<int>elsie;vector<int> bessie;
    for(int i  =0;i<n;i++){
        int x;
        fin>>x;
        elsie.push_back(x);
        total[x] =1;
    }
    for(int i = 1;i<=2*n;i++){
        if(total[i] ==0) bessie.push_back(i);
    }
    sort(elsie.begin(), elsie.end());
    sort(bessie.begin(), bessie.end());
    int j = 0;
    int ans =0;
    for(int i=0;i<elsie.size();i++){
        while(bessie[j]<elsie[i] && j<n) j++;
        if(j == n)break;
        if(bessie[j]>elsie[i]) {ans++;j++;}
    }
    fout<<ans<<endl;
}
