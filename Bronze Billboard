#include <bits/stdc++.h>
using namespace std;
#define lli long long int
lli n, m,d,s;
long int a[2001][2001]  = {0};
int main()
{
    ios_base::sync_with_stdio(false);
    ifstream fin("billboard.in");
    ofstream fout("billboard.out");
    fin.tie(NULL);
    for(int z=0;z<2;z++){
        int x1,x2,y1,y2;
        fin>>x1>>y1>>x2>>y2;
        x1+= 1000;x2+=1000; y1+=1000; y2+=1000;

        for(int i=y1;i<y2;i++){
            for(int j = x1;j<x2;j++){
                a[i][j] = 1;

            }
        }

    }
    int x1,x2,y1,y2;
    fin>>x1>>y1>>x2>>y2;
    x1+= 1000;x2+=1000; y1+=1000; y2+=1000;

    for(int i=y1;i<y2;i++){
        for(int j = x1;j<x2;j++){
            a[i][j] = 0;
        }
    }
    int ans  = 0;
    for(int i =0;i<2001;i++){
        for(int j  =0;j<2001;j++)
            if(a[i][j]  ==1) ans++;
    }
    fout<<ans<<endl;



}
